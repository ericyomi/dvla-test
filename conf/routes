# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.VehicleRegistrationSearchController.index
POST    /search                     controllers.VehicleRegistrationSearchController.search
GET     /registrations              controllers.VehicleRegistrationSearchController.getRegistrations

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
